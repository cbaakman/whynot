<!DOCTYPE validators PUBLIC
        "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
        "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="spdbid">
        <field-validator type="requiredstring">
            <message key="form.error.validation.spdbid_missing"/>
        </field-validator>
	    <field-validator type="regex">
			<param name="expression">[0-9a-zA-Z]{4}</param>
			<message key="form.error.validation.spdbid_format" />
		</field-validator>
    </field>
</validators>
